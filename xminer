            reply_markup=[
                [
                    {
                        'text' : "–í–∫–ª—é—á–∏—Ç—å –∫–æ–ø–∞–Ω–∏–µ" if not self.get('mm') else "–í—ã–∫–ª—é—á–∏—Ç—å –∫–æ–ø–∞–Ω–∏–µ",
                        'callback' : self.ikop,
                    },
                    {
                        'text' : '‚è± –ó–∞–¥–µ—Ä–∂–∫–∞ –∫–æ–ø–∞–Ω–∏—è ',
